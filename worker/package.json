{
  "name": "shortlink-worker",
  "version": "1.0.0",
  "description": "URL Shortener Cloudflare Worker",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create shortlink-db",
    "db:migrate": "wrangler d1 migrations apply shortlink-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply shortlink-db"
  },
  "devDependencies": {
    "wrangler": "^4.28.0"
  }
}
